							<xs:documentation>Your Rival's better looking picture</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Rank" type="ex:Rank">
						<xs:annotation>
							<xs:documentation>Your Rival's Higher rank</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Name" type="xs:string">
						<xs:annotation>
							<xs:documentation>Your Rival's Famous Name</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Age" type="xs:string">
							<xs:annotation>
								<xs:documentation>Your Rival's Believed Age</xs:documentation>
							</xs:annotation>
					</xs:element>
					<xs:element name="Concept" type="xs:string">
						<xs:annotation>
							<xs:documentation>Your Rival's Cooler Concept</xs:documentation>
						</xs:annotation>
					</xs:element>
