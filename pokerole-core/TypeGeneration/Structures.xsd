<xs:schema id="Structures"
	targetNamespace="https://www.pokeroleproject.com/schemas/Structures.xsd"
	elementFormDefault="qualified"
	xmlns="https://www.pokeroleproject.com/schemas/Structures.xsd"
	xmlns:mstns="http://tempuri.org/https://www.pokeroleproject.com/schemas/Structures.xsd"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:ms="http://microsoft.com/wsdl/types"
	xmlns:ex="https://www.pokeroleproject.com/schemas/ExternalTypes.xsd"
>
	<xs:import schemaLocation="MissingTypes.xsd" namespace="http://microsoft.com/wsdl/types"/>
	<xs:import schemaLocation="ExternalTypes.xsd" namespace="https://www.pokeroleproject.com/schemas/ExternalTypes.xsd"
			   />
	<xs:element name="PokeroleData">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="CustomTypes">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CustomType" type="ex:ITypeDefinition" maxOccurs="unbounded" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Moves">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Move" type="Move" maxOccurs="unbounded" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Images">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ImageRef" type="ImageRef" maxOccurs="unbounded" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Items">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Item" type="Item" maxOccurs="unbounded" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="DexEntries">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="DexEntry" type="DexEntry" maxOccurs="unbounded" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Abilities">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Ability" type="Ability" maxOccurs="unbounded" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="EvolutionTrees">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Tree" type="EvolutionTree" maxOccurs="unbounded" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="MonInstances">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="MonInstance" type="MonInstance" maxOccurs="unbounded" minOccurs="0"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>

				<!--<xs:element name="DexEntry" type="DexEntry" maxOccurs="unbounded" minOccurs="0"/>-->
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="Move">
		<xs:complexContent>
			<xs:extension base="ex:BaseDataItem">
				<xs:sequence>
					<xs:element name="Name" type="xs:string">
						<xs:annotation>
							<xs:documentation>Name of the move</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Description" type="xs:string">
						<xs:annotation>
							<xs:documentation>Move's description</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Power" type="xs:int">
						<xs:annotation>
							<xs:documentation>The power of the move</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MoveCategory" type="ex:MoveCategory">
						<xs:annotation>
							<xs:documentation>Category of the move</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Type" type="ex:ItemReference"  ex:genericType="ex:ITypeDefinition">
						<xs:annotation>
							<xs:documentation>Move Type</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="MoveTarget" type="ex:MoveTarget">
						<xs:annotation>
							<xs:documentation>What this move targets</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Ranged" type="xs:boolean">
						<xs:annotation>
							<xs:documentation>Whether or not this move is ranged</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PrimaryAccuracyStat" type="ex:ItemReference" ex:genericType="ex:IStat">
						<xs:annotation>
							<xs:documentation>Primary stat used for rolling accuracy</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PrimaryAccuracyIsNegative" type="xs:boolean">
						<xs:annotation>
							<xs:documentation>If true, points missing in the primary stat are used for accuracy</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SecondaryAccuracyStat" type="ex:ItemReference" ex:genericType="ex:IStat">
						<xs:annotation>
							<xs:documentation>Secondary stat used for rolling accuracy</xs:documentation>
						</xs:annotation>
					</xs:element>
					<!--<xs:element name="Accuracy" ex:listItemType="ex:ItemReference" ex:genericType="ex:IStat">
						<xs:annotation>
							<xs:documentation>Stats used to roll accuracy for this move</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Stat" maxOccurs="unbounded" type="ex:ItemReference" ex:genericType="ex:IStat" minOccurs="1"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>-->
					<xs:element name="ReducedAccuracy" type="xs:int">
						<xs:annotation>
							<xs:documentation>How many more successes are needed for this attack to hit</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DamageStat" type="ex:ItemReference" ex:genericType="ex:IStat" nillable="true">
						<xs:annotation>
							<xs:documentation>Stat used to roll damage for this move if any</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SecondaryDamageStat" type="ex:ItemReference" ex:genericType="ex:IStat" nillable="true">
						<xs:annotation>
							<xs:documentation>Stat used to roll additional damage for this move if any</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SecondaryDamageIsNegative" type="xs:boolean">
						<xs:annotation>
							<xs:documentation>If true, points missing in the secondary stat are used for damage</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DamageModifier" type="xs:int">
						<xs:annotation>
							<xs:documentation>How many more dice to add to the damage roll pool</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="HasSpecialAccuracyMod" type="xs:boolean">
						<xs:annotation>
							<xs:documentation>Refer to AdditionalInfo if this is true</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="HasSpecialDamageMod" type="xs:boolean">
						<xs:annotation>
							<xs:documentation>Refer to AdditionalInfo if this is true</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AdditionalInfo" type="xs:string">
						<xs:annotation>
							<xs:documentation>More information about this move that could not be contained in the other variables</xs:documentation>
						</xs:annotation>
					</xs:element>
				<xs:element name="Effects" ex:listItemType="xs:string">
					<xs:annotation>
						<xs:documentation>List of effects this move causes when it hits</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="Effect" type="xs:string" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="Item">
		<xs:complexContent>
			<xs:extension base="ex:BaseDataItem">
				<xs:sequence>
					<xs:element name="Name" type="xs:string">
						<xs:annotation>
							<xs:documentation>Item Name</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Description" type="xs:string">
						<xs:annotation>
							<xs:documentation>Item Description</xs:documentation>
						</xs:annotation>
					</xs:element>
					<!--<xs:element name="TypeBonus" ex:genericType="ex:ITypeDefinition" ex:listItemType="ex:ItemReference">
						<xs:annotation>
							<xs:documentation>Types this item gives bonus damage to, if any</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Type" maxOccurs="unbounded" minOccurs="0" type="ex:ItemReference"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="TypeBonusValue" nillable="true" type="xs:int">
						<xs:annotation>
							<xs:documentation>Bonus damage this item grants for the given type(s) if any</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="StrengthBoost" nillable="true" type="xs:int">
						<xs:annotation>
							<xs:documentation>The boost this item confers upone the Strength stat if any</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DexterityBoost" nillable="true" type="xs:int">
						<xs:annotation>
							<xs:documentation>The boost this item confers upone the Dexterity stat if any</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="VitalityBoost" nillable="true" type="xs:int">
						<xs:annotation>
							<xs:documentation>The boost this item confers upone the Vitality stat if any</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SpecialBoost" nillable="true" type="xs:int">
						<xs:annotation>
							<xs:documentation>The boost this item confers upone the Special stat if any</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="InsightBoost" nillable="true" type="xs:int">
						<xs:annotation>
							<xs:documentation>The boost this item confers upone the Insight stat if any</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DefenseBoost" nillable="true" type="xs:int">
						<xs:annotation>
							<xs:documentation>The boost this item confers upone the Defense stat if any</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EvasionBoost" nillable="true" type="xs:int">
						<xs:annotation>
							<xs:documentation>The boost this item confers upone the Evasion stat if any</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AccuracyBoost" nillable="true" type="xs:int">
						<xs:annotation>
							<xs:documentation>The boost this item confers upone the Accuracy stat if any</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SpecificPokemon" ex:genericType="DexEntry" ex:listItemType="ex:ItemReference">
						<xs:annotation>
							<xs:documentation>List of pokemon this item is exclusive to if any</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="DexEntry" maxOccurs="unbounded" minOccurs="0" type="ex:ItemReference"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="HealAmount" nillable="true" type="xs:int">
						<xs:annotation>
							<xs:documentation>Hitpoints this item heals if any</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="HealedStatusConditions" ex:genericType="DexEntry" ex:listItemType="ex:ItemReference">
						<xs:annotation>
							<xs:documentation>List of pokemon this item is exclusive to if any</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="DexEntry" maxOccurs="unbounded" minOccurs="0" type="ex:ItemReference"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="ReviveType" type="ex:ReviveType" nillable="true">
						<xs:annotation>
							<xs:documentation>Kind of revive this item is if it is one</xs:documentation>
						</xs:annotation>
					</xs:element>-->
					
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CustomStatEntry">
		<xs:attribute name="Stat" type="xs:string"/>
		<xs:attribute name="Value" type="xs:int"/>
	</xs:complexType>
	<xs:complexType name="ImageRef">
		<xs:complexContent>
			<xs:extension base="ex:BaseDataItem">
				<xs:sequence>
					<xs:element name="Filename" type="xs:string"/>
					<xs:element name="Data" type="xs:base64Binary" nillable="true"/>
					<xs:element name="FilePath" type="xs:string" nillable="true"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="MegaEvolutionEntry">
		<xs:sequence>
			<xs:element name="Item" type="ex:ItemReference" ex:genericType="Item">
				<xs:annotation>
					<xs:documentation>Item the Pokémon must be holding to perform this megaevolution</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TargetEvolution" type="ex:ItemReference" ex:genericType="DexEntry">
				<xs:annotation>
					<xs:documentation>DexEntry to use when mega-evolved</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MoveEntry">
		<xs:sequence>
			<xs:element name="Move" type="ex:ItemReference" ex:genericType="Move"/>
		</xs:sequence>
		<xs:attribute name="Rank" type="ex:Rank">
			<xs:annotation>
				<xs:documentation>Required rank to learn the given move</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="AbilityEntry">
		<xs:sequence>
			<xs:element name="Ability" type="ex:ItemReference" ex:genericType="Ability"/>
		</xs:sequence>
		<xs:attribute name="Hidden" type="xs:boolean" default="false">
			<xs:annotation>
				<xs:documentation>Whether or not this is a hidden ability</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="DexEntry">
		<xs:complexContent>
			<xs:extension base="ex:BaseDataItem">
				<xs:sequence>
					<xs:element name="DexNum" type="xs:int">
						<xs:annotation>
							<xs:documentation>The international dex number of this Pokémon</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SuggestedStarter" type="xs:boolean">
						<xs:annotation>
							<xs:documentation>Whether or not this Pokémon is recommended as a starter</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PrimaryType" type="ex:ItemReference" ex:genericType="ex:ITypeDefinition">
						<xs:annotation>
							<xs:documentation>The primary type of this Pokémon</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SecondaryType" type="ex:ItemReference" ex:genericType="ex:ITypeDefinition" nillable="true">
						<xs:annotation>
							<xs:documentation>The secondary type of this Pokémon if applicable</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Name" type="xs:string">
						<xs:annotation>
							<xs:documentation>The name of this Pokémon</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Variant" type="xs:string" nillable="true">
						<xs:annotation>
							<xs:documentation>Regional variant of this Pokémon (if applicable) such as "Galaran"</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AverageHeight" type="Height">
						<xs:annotation>
							<xs:documentation>The average height of this Pokémon</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AverageWeight" type="Weight">
						<xs:annotation>
							<xs:documentation>The average weight of this Pokémon</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Category" type="xs:string">
						<xs:annotation>
							<xs:documentation>The descriptive category for this Pokémon</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DexDescription" type="xs:string">
						<xs:annotation>
							<xs:documentation>The description of this Pokémon</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SuggestedRank" type="ex:Rank">
						<xs:annotation>
							<xs:documentation>Suggested starting rank of this Pokémon</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BaseHp" type="xs:int">
						<xs:annotation>
							<xs:documentation>The base hp of this Pokémon</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PrimaryImage" type="ex:ItemReference" ex:genericType="ImageRef">
						<xs:annotation>
							<xs:documentation>Primary display image of this Pokémon</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AdditionalImages" ex:listItemType="ItemReference"
								ex:genericType="ImageRef">
						<xs:annotation>
							<xs:documentation>Additional images of this pokemon</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="ItemReference" type="ex:ItemReference" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="AdditionalShinyImages" ex:listItemType="ItemReference"
								ex:genericType="ImageRef">
						<xs:annotation>
							<xs:documentation>Additional shiny images of this pokemon</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="ItemReference" type="ex:ItemReference" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="SpriteImage" type="ex:ItemReference" ex:genericType="ImageRef">
						<xs:annotation>
							<xs:documentation>Smaller sprite image of this Pokémon</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ShinyImage" type="ex:ItemReference" ex:genericType="ImageRef" nillable="true">
						<xs:annotation>
							<xs:documentation>Primary display image of a shiny instance of this Pokémon</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="IsBaby" type="xs:boolean">
						<xs:annotation>
							<xs:documentation>Is this a baby pokemon?</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SpriteShinyImage" type="ex:ItemReference" ex:genericType="ImageRef" nillable="true">
						<xs:annotation>
							<xs:documentation>Smaller sprite image of a shiny instance of this Pokémon</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PrimaryFemaleImage" type="ex:ItemReference" ex:genericType="ImageRef" nillable="true">
						<xs:annotation>
							<xs:documentation>Primary female display image of this Pokémon</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AdditionalFemaleImages" ex:listItemType="ItemReference"
								ex:genericType="ImageRef">
						<xs:annotation>
							<xs:documentation>Additional female images of this pokemon</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="ItemReference" type="ex:ItemReference" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="AdditionalShinyFemaleImages" ex:listItemType="ItemReference"
								ex:genericType="ImageRef">
						<xs:annotation>
							<xs:documentation>Additional shiny female images of this pokemon</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="ItemReference" type="ex:ItemReference" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="SpriteFemaleImage" type="ex:ItemReference" ex:genericType="ImageRef" nillable="true">
						<xs:annotation>
							<xs:documentation>Smaller female display image of this Pokémon</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ShinyFemaleImage" type="ex:ItemReference" ex:genericType="ImageRef" nillable="true">
						<xs:annotation>
							<xs:documentation>Shiny female display image of a shiny instance of this Pokémon</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SpriteShinyFemaleImage" type="ex:ItemReference" ex:genericType="ImageRef" nillable="true">
						<xs:annotation>
							<xs:documentation>Sprite female display image of a shiny instance of this Pokémon</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="AdditionalSpriteImages" ex:listItemType="ItemReference"
								ex:genericType="ImageRef">
						<xs:annotation>
							<xs:documentation>Additional sprite images of this pokemon</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="ItemReference" type="ex:ItemReference" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="AdditionalShinySpriteImages" ex:listItemType="ItemReference"
								ex:genericType="ImageRef">
						<xs:annotation>
							<xs:documentation>Additional shiny sprite images of this pokemon</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="ItemReference" type="ex:ItemReference" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="AdditionalFemaleSpriteImages" ex:listItemType="ItemReference"
								ex:genericType="ImageRef">
						<xs:annotation>
							<xs:documentation>Additional sprite female images of this pokemon</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="ItemReference" type="ex:ItemReference" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="AdditionalShinyFemaleSpriteImages" ex:listItemType="ItemReference"
								ex:genericType="ImageRef">
						<xs:annotation>
							<xs:documentation>Additional shiny sprite female images of this pokemon</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="ItemReference" type="ex:ItemReference" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:element name="Abilities" ex:listItemType="AbilityEntry">
						<xs:annotation>
							<xs:documentation>List of possible abilities this Pokémon can have</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="AbilityEntry" type="AbilityEntry" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
