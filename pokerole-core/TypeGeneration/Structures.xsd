	<xs:complexType name="EvolutionEntry">
		<xs:sequence>
			<xs:element name="From" type="ex:ItemReference" ex:genericType="DexEntry">
				<xs:annotation>
					<xs:documentation>What it is being evolved from</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="To" type="ex:ItemReference" ex:genericType="DexEntry">
				<xs:annotation>
					<xs:documentation>What it is being evolved to</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Kind" type="ex:EvolutionKind">
				<xs:annotation>
					<xs:documentation>Type of evolution (for quick filtering)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Details" type="xs:string">
				<xs:annotation>
					<xs:documentation>Further details about how the evolution works in JSON. This could be a level number, name of an item, or just about anything really...</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EvolutionTree">
		<xs:complexContent>
			<xs:extension base="ex:BaseDataItem">
				<!--TODO: Implement-->
				<xs:sequence>
					<xs:element name="Name" type="xs:string">
						<xs:annotation>
							<xs:documentation>Name of this Evolution list. Typically the root 'mon's species name followed by " Line"</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BabyEvolutionItem" type="ex:ItemReference" ex:genericType="Item" nillable="true">
						<xs:annotation>
							<xs:documentation>An what item a parent must be holding to get the baby root of this </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BreedCounterpart" type="ex:ItemReference" ex:genericType="DexEntry" nillable="true">
						<xs:annotation>
							<xs:documentation>Breedable counterpart for this evolution tree if any. The most obvious example is the Nidoran pair. The value will be the root of the couterpart's evolution tree</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BabyItem" type="ex:ItemReference" ex:genericType="Item" nillable="true">
						<xs:annotation>
							<xs:documentation>Type of item a parent must be holding in order to get the baby root of this tree instead of NonBabyRoot, if any</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="NonBabyRoot" type="ex:ItemReference" ex:genericType="DexEntry" nillable="true">
						<xs:annotation>
							<xs:documentation>Egg result if this tree has a BabyItem and neither parent was holding it</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Root" type="ex:ItemReference" ex:genericType="DexEntry">
						<xs:annotation>
							<xs:documentation>Root entry of this evolution tree</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EvolutionEntries" ex:listItemType="EvolutionEntry">
						<xs:annotation>
							<xs:documentation>Evolution entries of this tree</xs:documentation>
						</xs:annotation>
						<xs:complexType>
							<xs:sequence>
								<xs:element name="Entry" type="EvolutionEntry" maxOccurs="unbounded"/>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
